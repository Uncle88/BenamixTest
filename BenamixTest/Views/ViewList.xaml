<?xml version="1.0" encoding="UTF-8"?>
<views:BaseView xmlns="http://xamarin.com/schemas/2014/forms" 
                xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
                x:Class="BenamixTest.Views.ViewList"
                xmlns:views="clr-namespace:BenamixTest.Views;assembly=BenamixTest">
    <ContentPage.Content>
        <ListView ItemsSource="{Binding ResponceList}">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <ViewCell>
                        <StackLayout Orientation="Horizontal">
                            <Label Text="{Binding bids}"/>
                            <Label Text="{Binding asks}"/>
                        </StackLayout>
                    </ViewCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </ContentPage.Content>
</views:BaseView>
